-- ==============================
-- –£—Ç–∏–ª–∏—Ç—ã
-- ==============================
local function safeLower(str)
	if typeof(str) ~= "string" then return "" end
	return string.lower(str)
end

local function stringContainsAny(haystack, needles)
	haystack = safeLower(haystack)
	for _, needle in ipairs(needles) do
		if string.find(haystack, needle, 1, true) then return true end
	end
	return false
end

-- ==============================
-- –ù–∞–¥—ë–∂–Ω–∞—è —Å–µ—Ä–∏–∞–ª–∏–∑–∞—Ü–∏—è –≤ JSON
-- ==============================
-- –†–µ–∫—É—Ä—Å–∏–≤–Ω–æ –ø—Ä–∏–≤–æ–¥–∏—Ç –¥–∞–Ω–Ω—ã–µ –∫ "–±–µ–∑–æ–ø–∞—Å–Ω—ã–º" —Ç–∏–ø–∞–º: table/string/number/boolean –∏–ª–∏ tostring(–∏–Ω–∞—á–µ)
local function sanitizeForJson(value, seen, depth)
	depth = depth or 0
	if depth > 8 then
		-- –∑–∞—â–∏—Ç–Ω—ã–π –ø—Ä–µ–¥–µ–ª –≥–ª—É–±–∏–Ω—ã
		return tostring(value)
	end

	local t = typeof(value)
	if t == "table" then
		-- –∑–∞—â–∏—Ç–∏–º—Å—è –æ—Ç —Ü–∏–∫–ª–æ–≤
		seen = seen or {}
		if seen[value] then
			return "[cyclic]"
		end
		seen[value] = true
		local out = {}
		for k, v in pairs(value) do
			-- –∫–ª—é—á–∏ –≤ JSON –¥–æ–ª–∂–Ω—ã –±—ã—Ç—å —Å—Ç—Ä–æ–∫–∏
			local keyType = typeof(k)
			local key
			if keyType == "string" then
				key = k
			elseif keyType == "number" or keyType == "boolean" then
				key = tostring(k)
			else
				key = tostring(k)
			end
			out[key] = sanitizeForJson(v, seen, depth + 1)
		end
		return out
	elseif t == "string" or t == "number" or t == "boolean" then
		return value
	elseif t == "RBXScriptSignal" or t == "Instance" then
		-- –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏–µ –¥–ª—è Roblox-–æ–±—ä–µ–∫—Ç–æ–≤
		return tostring(value)
	else
		-- –¥–ª—è —Ñ—É–Ω–∫—Ü–∏–π, userdata, thread, proxy –∏ —Ç.–¥. ‚Äî —Å—Ç—Ä–æ–∫–æ–≤–æ–µ –ø—Ä–µ–¥—Å—Ç–∞–≤–ª–µ–Ω–∏–µ
		-- –æ–±—Ñ—É—Å–∫–∞—Ç–æ—Ä—ã –∏–Ω–æ–≥–¥–∞ –≤—Å—Ç–∞–≤–ª—è—é—Ç proxy-–æ–±—ä–µ–∫—Ç—ã/closures ‚Äî tostring –∏—Ö –±–µ–∑–æ–ø–∞—Å–µ–Ω
		return tostring(value)
	end
end

local function safeJSONEncode(payload)
	-- —Å–Ω–∞—á–∞–ª–∞ –ø–æ–ø—ã—Ç–∫–∞ —Å–µ—Ä–∏–∞–ª–∏–∑–æ–≤–∞—Ç—å –ø—Ä—è–º–æ (–±—ã–≤–∞–µ—Ç, —á—Ç–æ payload —É–∂–µ "—á–∏—Å—Ç—ã–π")
	local ok, encoded = pcall(function()
		return HttpService:JSONEncode(payload)
	end)
	if ok then
		return encoded
	end

	-- –ø–µ—Ä–≤–æ–µ –Ω–µ—É–¥–∞—á–Ω–æ–µ –∫–æ–¥–∏—Ä–æ–≤–∞–Ω–∏–µ ‚Äî –ø–æ–ø—Ä–æ–±—É–µ–º –ø—Ä–µ–¥–≤–∞—Ä–∏—Ç–µ–ª—å–Ω–æ "–æ—á–∏—Å—Ç–∏—Ç—å" —Ç–∞–±–ª–∏—Ü—É
	local sanitized = sanitizeForJson(payload)
	ok, encoded = pcall(function()
		return HttpService:JSONEncode(sanitized)
	end)
	if ok then
		return encoded
	end

	-- –µ—Å–ª–∏ –∏ —ç—Ç–æ –Ω–µ –ø–æ–º–æ–≥–ª–æ ‚Äî –≤–æ–∑–≤—Ä–∞—â–∞–µ–º –º–∏–Ω–∏–º–∞–ª—å–Ω—É—é –±–µ–∑–æ–ø–∞—Å–Ω—É—é –∑–∞–≥–ª—É—à–∫—É
	warn("[safeJSONEncode] Failed to JSONEncode payload. Falling back to simple content. Error:", encoded)
	local fallback = { content = tostring(payload) }
	local ok2, encoded2 = pcall(function()
		return HttpService:JSONEncode(fallback)
	end)
	if ok2 then
		return encoded2
	end

	-- –ø–æ—Å–ª–µ–¥–Ω—è—è –ø–æ–ø—ã—Ç–∫–∞: –≤—Ä—É—á–Ω—É—é —Å–æ–±—Ä–∞—Ç—å –æ—á–µ–Ω—å –ø—Ä–æ—Å—Ç—É—é JSON-—Å—Ç—Ä–æ–∫—É
	return '{"content":' .. HttpService:JSONEncode(tostring(payload)) .. '}'
end

-- ==============================
-- Webhook (—É—Å—Ç–æ–π—á–∏–≤—ã–µ –∑–∞–ø—Ä–æ—Å—ã)
-- ==============================
local function tryExploitRequest(json)
	local req = (syn and syn.request) or (http and http.request) or (http_request) or (request)
	if typeof(req) == "function" then
		-- –û–±—ë—Ä–Ω—É –≤ pcall, —á—Ç–æ–±—ã –Ω–∏ –Ω–∞ —á—Ç–æ –Ω–µ –ø–∞–¥–∞–ª–æ
		local ok, err = pcall(function()
			req({
				Url = WEBHOOK_URL,
				Method = "POST",
				Headers = { ["Content-Type"] = "application/json" },
				Body = json,
			})
		end)
		if not ok then
			warn("[tryExploitRequest] request failed:", err)
			return false
		end
		return true
	end
	return false
end

local function tryHttpServiceRequest(json)
	local ok, res = pcall(function()
		-- RequestAsync –º–æ–∂–µ—Ç –≤—ã–±—Ä–æ—Å–∏—Ç—å –æ—à–∏–±–∫—É, –ø–æ—ç—Ç–æ–º—É pcall
		return HttpService:RequestAsync({
			Url = WEBHOOK_URL,
			Method = "POST",
			Headers = { ["Content-Type"] = "application/json" },
			Body = json,
		})
	end)
	if not ok then
		warn("[tryHttpServiceRequest] RequestAsync pcall failed:", res)
		return false
	end
	-- res –º–æ–∂–µ—Ç –±—ã—Ç—å nil –∏–ª–∏ –∏–º–µ—Ç—å –¥—Ä—É–≥–æ–µ –ø–æ–ª–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –æ–±—Ñ—É—Å–∫–∞—Ç–æ—Ä–∞/—Å–∫—Ä–∏–ø—Ç–∞ —Å—Ä–µ–¥—ã
	if type(res) == "table" and (res.Success == true or res.success == true) then
		return true
	end
	-- –í –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö –æ–∫—Ä—É–∂–µ–Ω–∏—è—Ö res –º–æ–∂–µ—Ç –±—ã—Ç—å —Å—Ç—Ä–æ–∫–æ–π –∏–ª–∏ –¥—Ä—É–≥–æ–π —Å—Ç—Ä—É–∫—Ç—É—Ä–æ–π ‚Äî –ª–æ–≥–∏—Ä—É–µ–º –¥–ª—è –æ—Ç–ª–∞–¥–∫–∏
	warn("[tryHttpServiceRequest] Request returned:", res)
	return false
end

local function sendWebhook(payload)
	if not WEBHOOK_URL or WEBHOOK_URL == "" then return end
	-- –ï—Å–ª–∏ payload ‚Äî –Ω–µ —Ç–∞–±–ª–∏—Ü–∞, —Å–¥–µ–ª–∞–µ–º —Ç–∞–±–ª–∏—Ü—É —Å –∫–æ–Ω—Ç–µ–Ω—Ç–æ–º
	if type(payload) ~= "table" then
		payload = { content = tostring(payload) }
	end

	-- –ü–æ–¥–≥–æ—Ç–æ–≤–∏–º –±–µ–∑–æ–ø–∞—Å–Ω—ã–π JSON
	local json = safeJSONEncode(payload)

	-- –ü–æ–ø—Ä–æ–±—É–µ–º —á–µ—Ä–µ–∑ —ç–∫—Å–ø–ª–æ–π—Ç–Ω—ã–µ —Ö—É–∫–∏ (syn/request –∏ —Ç.–¥.)
	if tryExploitRequest(json) then
		return
	end

	-- –ü–æ–ø—Ä–æ–±—É–µ–º —á–µ—Ä–µ–∑ HttpService
	tryHttpServiceRequest(json)
end

local function postDiscordSimple(content)
	if not WEBHOOK_URL or WEBHOOK_URL == "" then return end
	-- –û–≥—Ä–∞–Ω–∏—á–µ–Ω–Ω–∞—è –ø–æ –¥–ª–∏–Ω–µ –æ—Ç–ø—Ä–∞–≤–∫–∞ (Discord message limit/HTML code block stuff)
	local MAX = 1800
	local i = 1
	while i <= #content do
		local chunk = string.sub(content, i, math.min(i + MAX - 1, #content))
		-- –∏—Å–ø–æ–ª—å–∑—É–µ–º sendWebhook, –æ–Ω –æ–±—Ä–∞–±–æ—Ç–∞–µ—Ç –≤—Å—ë –±–µ–∑–æ–ø–∞—Å–Ω–æ
		sendWebhook({ content = "```\n" .. chunk .. "\n```" })
		i = i + MAX
	end
end

